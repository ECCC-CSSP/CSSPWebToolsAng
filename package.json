{
  "name": "csspwebtoolsang",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "startaot": "ng serve --aot --proxy-config proxy.config.json",
    "startfr": "ng serve --proxy-config proxy.config.json --configuration=fr",
    "startaotfr": "ng serve --aot --proxy-config proxy.config.json --configuration=fr",
    "build": "ng build --prod",
    "buildfr": "ng build --prod --configuration=fr",
    "buildaot": "ng build --aot",
    "buildaotfr": "ng build --aot --configuration=production-fr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "typedoc --out ./doc --module commonjs --includeDeclarations --ignoreCompilerErrors --excludeExternals",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "xi18nfr": "ng xi18n --i18nFormat=xlf --i18nLocale=fr --output-path locale/fr --out-file messages.fr.xlf",
    "xi18nen": "ng xi18n --i18nFormat=xlf --i18nLocale=en --output-path locale/en --out-file messages.en.xlf"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^8.2.13",
    "@angular/compiler": "^8.2.13",
    "@angular/core": "^8.2.13",
    "@angular/forms": "^8.2.13",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^8.2.13",
    "@angular/platform-browser-dynamic": "^8.2.13",
    "@angular/router": "^8.2.13",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.5.3",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.18",
    "@angular/cli": "~8.3.18",
    "@angular/compiler-cli": "^8.2.13",
    "@angular/language-service": "^8.2.13",
    "@compodoc/compodoc": "^1.1.6",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.10",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typedoc": "^0.13.0",
    "typescript": "~3.5.3"
  }
}
